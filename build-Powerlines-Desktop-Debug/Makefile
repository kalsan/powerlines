####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/opt/Powerlines/bin || mkdir -p $(INSTALL_ROOT)/opt/Powerlines/bin
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/opt/Powerlines/bin/$(QMAKE_TARGET)"

uninstall_target: FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/Powerlines/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/Powerlines/bin/ 


install:  install_target  FORCE

uninstall: uninstall_target   FORCE

FORCE:

